{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "blogitems": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "oid": {
            "type": "string"
          },
          "title": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "oid",
          "title"
        ],
        "additionalProperties": false
      }
    },
    "count": {
      "type": "integer"
    }
  },
  "required": ["blogitems", "count"],
  "additionalProperties": false
}
